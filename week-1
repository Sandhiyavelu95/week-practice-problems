Problem 1:
//even or odd
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n=kbd.nextInt();
	    if(n%2==0)
	        System.out.print("even");
	    else
	         System.out.print("odd");
	}
}

  Problem 2:
//positive or negative
   import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n=kbd.nextInt();
	    if(n>0)
	    {
	        System.out.print("positive");
	    }
	    else if(n<0)
	    {
	        System.out.print("negative");
	    }
	    else
	    {
	        System.out.print("zero");
	
	    }
	}
}

 Problem 3:
    //divisible by 3 and 5
 import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n=kbd.nextInt();
	    if(n%3==0 && n%5==0)
	    System.out.print("yes");
	    else
	    System.out.print("no");
	}
}

 Problem 4:
   // Convert Days into Years, Months, Days
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int n =1332;
	    int m = n%365;
	    int d= m%30;
	    if(n>0)
	    {
	    int year=n/365;
	    System.out.println("years:"+year);
	    }
	    
	    if(m>0)
	    {
	       m = m/30;
	      System.out.println("months :"+m);
	     
	    }
	     
	    if(d>0)
	    {
	        System.out.println("days :"+d);
	    }
	}
}

 Problem 5:
    //Mileage Remuneration Calculator
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    double start=kbd.nextDouble();
	    double end=kbd.nextDouble();
	    double dis = end-start;
	    double rem = dis*25;
	      System.out.printf("%.2f %.2f",dis,rem);
	}
}

 Problem 6:
    // print Max & Min 
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int a=kbd.nextInt();
	    int b=kbd.nextInt();
	    if(a>b){
	     System.out.println("Max :"+a);
	      System.out.println("Min :"+b);
	}
	}
}

 Problem 7:
   //print  Max of 3 Numbers
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner kbd = new Scanner(System.in);
        int a = kbd.nextInt();
        int b = kbd.nextInt();
        int c = kbd.nextInt();
        
        // Find maximum
        int max = a;
        if (b > max) {
            max = b;
        }
        if (c > max) {
            max = c;
        }
        System.out.println("Max num:" + max);
    }
}

