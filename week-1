 Problem 20:
  //Count Digits
  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n = kbd.nextInt();
	    int count=0;
        while(n>0)
        {
            n=n/10;
            count++;
            
        }
        System.out.print(count);
	}
}

 Problem 21:
  // Sum of Digits 
  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n = kbd.nextInt();
	    int count=0;
	    int sum=0;
        while(n>0)
        {
            int dig=n%10;
            sum+=dig;
            n=n/10;
        }
        
        System.out.print(sum);
	}
}

 Problem 22:
  //Harshad Number
  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n = kbd.nextInt();
        int sum=0;
        while(n>0)
        {
            int dig = n%10;
             sum = sum+dig;
             n = n/10;
        }
        if(n%sum==0)
        System.out.print("Harshad Number");
        else
         System.out.print("not Harshad Number");
	}
}

 Problem 23:
  //Perfect Number 
  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int num = kbd.nextInt();
	    int sum=0;
	    for(int i=1;i<=num/2;i++)
	    if(num%i==0){
	        sum+=i;
	    }
	    if(sum==num)
	     System.out.print("Perfect Number");
	     else
	     System.out.print("Not Perfect Number");
	    }
}

 Problem 24:
  // Prime Number
  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int num = kbd.nextInt();
	    if(num%1==0 || num%num==0)
	    System.out.print("Prime Number");
	     else
	     System.out.print("Not Prime Number");
	}
}

 Problem 25:
  // Perfect Square 
  import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int num = kbd.nextInt();
	     int sqrt = (int) Math.sqrt(num);
	     if(sqrt*sqrt==num)
	     System.out.print("Perfect square");
	     else
	     System.out.print("Not Perfect square");
	    }
}

 Problem 26:
  //Fibonacci Series 
 import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	    Scanner kbd=new Scanner(System.in);
	    int n= kbd.nextInt();
	    int fir=0, sec=1;
        for (int i = 1; i <= n; i++) {
            System.out.print(fir + " ");
            int temp = fir + sec;
            fir = sec;
            sec = temp;
        }
	}
}

	     

	    
  
	    
	    
        
    
        
    

        
        
        
        


  

	    
